{
	"builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
	"redirects": [
		{
			"source": "/",
			"has": [
				{
					"type": "query",
					"key": "page_id",
					"value": "12"
				}
			],
			"destination": "/about",
			"permanent": true
		}
	]
}
